        "newerton/yii2-fancybox-3": "dev-master",
        "omgdef/yii2-multilingual-behavior": "~2.0",
        "sunhater/kcfinder": "dev-master",
        "sweelix/plupload": "dev-master",
        "sweelix/yii2-plupload": "dev-master",
        "pudinglabs/yii2-bootstrap-tags-input": "*",
        "kartik-v/yii2-slider": "dev-master",
        "yii2assets/yii2-pdfjs": ">=1.0",
        "moonlandsoft/yii2-phpexcel": "*",
        "paypal/rest-api-sdk-php": "^1.14",
        "guzzlehttp/guzzle": "^7.0",
        "unclead/yii2-multiple-input": "~2.0",
        "almasaeed2010/adminlte": "^2.4",
        "twilio/sdk": "^6.32"
